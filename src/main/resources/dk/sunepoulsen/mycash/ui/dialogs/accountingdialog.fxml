<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root type="javafx.scene.layout.GridPane" xmlns:fx="http://javafx.com/fxml" hgap="10" vgap="10">
    <Label text="%label.name" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
    <TextField fx:id="nameField" GridPane.columnIndex="1" GridPane.rowIndex="0"/>

    <Label text="%label.start.date" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
    <DatePicker fx:id="startDate" GridPane.columnIndex="1" GridPane.rowIndex="1"/>

    <Label text="%label.end.date" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
    <DatePicker fx:id="endDate" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
</fx:root>
